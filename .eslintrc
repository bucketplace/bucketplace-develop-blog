{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "no-console": "warn",
    "quotes": [2, "single", { "avoidEscape": true }],
    "no-unused-vars": [
      1
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "16.8.0"
    }
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": ["prettier", "jsx-a11y"]
}
